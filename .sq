sq: 0,
git: 'sq',
require: [
  'git',
  'xcodebuild'
],
bin: {
  build: {
    info: 'build sq binary',
    run: 'xcodebuild clean build -project sq.xcodeproj -arch arm64 -scheme sq -configuration #config -> Debug# -derivedDataPath ./xcode'
  },
  clean: {
    info: 'delete untracked files',
    run: 'git clean -dfxi -e .env -e xcschemes'
  }
}
